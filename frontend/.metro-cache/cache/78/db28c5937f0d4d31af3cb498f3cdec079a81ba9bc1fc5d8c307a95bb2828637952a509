–∞–¥–∞–Ω–Ω–æ–π –≤—ã—Å–æ—Ç–æ–π —Ç–æ–Ω–∞\n     */\n    async playBeepWithPitch(pitch) {\n      try {\n        if (!this.beepSound) {\n          await this.initBeepSound();\n        }\n        if (this.beepSound) {\n          await this.beepSound.setPositionAsync(0);\n          await this.beepSound.setRateAsync(pitch, true); // –ò–∑–º–µ–Ω—è–µ–º –≤—ã—Å–æ—Ç—É —Ç–æ–Ω–∞\n          await this.beepSound.setVolumeAsync(this.settings.volume);\n          await this.beepSound.playAsync();\n        }\n      } catch (error) {\n        console.error('‚ùå Error playing beep with pitch:', error);\n      }\n    }\n\n    /**\n     * –û—á–∏—Å—Ç–∫–∞ —Ä–µ—Å—É—Ä—Å–æ–≤\n     */\n    async cleanup() {\n      if (this.beepSound) {\n        await this.beepSound.unloadAsync();\n        this.beepSound = null;\n      }\n    }\n  }\n  var _default = new DynamicAudioAlertService();\n});","lineCount":400,"map":[[27,2,406,0,"Object"],[27,8,406,0],[27,9,406,0,"defineProperty"],[27,23,406,0],[27,24,406,0,"exports"],[27,31,406,0],[28,4,406,0,"enumerable"],[28,14,406,0],[29,4,406,0,"get"],[29,7,406,0],[29,18,406,0,"get"],[29,19,406,0],[30,6,406,0],[30,13,406,0,"_default"],[30,21,406,0],[31,4,406,0],[32,2,406,0],[33,2,29,0,"Object"],[33,8,29,0],[33,9,29,0,"defineProperty"],[33,23,29,0],[33,24,29,0,"exports"],[33,31,29,0],[34,4,29,0,"enumerable"],[34,14,29,0],[35,4,29,0,"get"],[35,7,29,0],[35,18,29,0,"get"],[35,19,29,0],[36,6,29,0],[36,13,29,0,"DEFAULT_RECOMMENDED_SPEEDS"],[36,39,29,0],[37,4,29,0],[38,2,29,0],[39,2,12,0],[39,6,12,0,"_expoAv"],[39,13,12,0],[39,16,12,0,"require"],[39,23,12,0],[39,24,12,0,"_dependencyMap"],[39,38,12,0],[40,2,13,0],[40,6,13,0,"_expoSpeech"],[40,17,13,0],[40,20,13,0,"require"],[40,27,13,0],[40,28,13,0,"_dependencyMap"],[40,42,13,0],[41,2,13,0],[41,6,13,0,"Speech"],[41,12,13,0],[41,15,13,0,"_interopNamespace"],[41,32,13,0],[41,33,13,0,"_expoSpeech"],[41,44,13,0],[42,2,14,0],[42,6,14,0,"_reactNativeAsyncStorageAsyncStorage"],[42,42,14,0],[42,45,14,0,"require"],[42,52,14,0],[42,53